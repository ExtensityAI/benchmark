    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {"PYTHONPATH": "${workspaceFolder}"}
        },
        {
            "name": "Run Context Associations",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--context_associations",
                "--models", "gpt3.5",
                "--seeds", "42"
            ]
        },
        {
            "name": "Run Context Multimodal",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--multimodal_bindings",
                "--models", "gpt3.5",
                "--seeds", "42"
            ]
        },
        {
            "name": "Run Program Synthesis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--program_synthesis",
                "--models", "gpt3.5",
                "--seeds", "42"
            ]
        },
        {
            "name": "Run Logic Components",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--logic_components",
                "--models", "gpt3.5",
                "--seeds", "42"
            ]
        },
        {
            "name": "Run Computation Graphs",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--computation_graphs",
                "--models", "gpt3.5",
                "--seeds", "42"
            ]
        },
        {
            "name": "Multi-Run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--context_associations",
                "--program_synthesis",
                "--multimodal_bindings",
                "--logic_components",
                "--computation_graphs"
            ]
        },
        {
            "name": "Run Dummy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--dummy"
            ]
        }
    ]
}