    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Run Context Associations",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--context_associations"
            ]
        },
        {
            "name": "Run Context Multimodal",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--multimodal_bindings"
            ]
        },
        {
            "name": "Run Program Synthesis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--program_synthesis"
            ]
        },
        {
            "name": "Run Dummy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--dummy"
            ]
        }
    ]
}